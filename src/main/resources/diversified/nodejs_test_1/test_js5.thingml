datatype Byte<1>
	@type_checker "Byte"
	@java_type "byte"
datatype Integer<2>
	@type_checker "Integer"
	@java_type "short"
datatype Long<4>
	@type_checker "Integer"
	@java_type "int"
datatype LongLong<8>
	@type_checker "Integer"
	@java_type "long"
thing fragment Msgs {
	message m2(c : Integer, a : Byte @upsize "not", b : Byte @upsize "not")
	@code "0x24"
	message m1(d : LongLong @weakparam "true", a : Byte @upsize "not", b : Byte @upsize "not", c : Integer, e : Byte)
	@code "0x25"
	message m3(a : LongLong)
	@code "0x26"
	message m2_()
	@code "0x27"
	message m2cab(c : Integer, a : Byte @upsize "not", b : Byte @upsize "not")
	@code "0x28"
	message m1d(d : LongLong @weakparam "true")
	@code "0x29"
	message m1abce(a : Byte @upsize "not", b : Byte @upsize "not", c : Integer, e : Byte)
	@code "0x2A"
	message m3_()
	@code "0x2B"
	message m3a(a : LongLong)
	@code "0x2C"
	function rnd() : Byte do
		return `Math.floor(Math.random() * Math.floor(256))`
	end
}
thing App includes Msgs {
	provided port app {
		sends m3_ sends m3a receives m2_ receives m2cab receives m1d receives m1abce
	}
	internal port diversified {
		sends m2 sends m1d sends m1abce sends m1 receives m2 receives m1d receives m1abce receives m1
	}
	property id : Byte
	statechart init WaitForM1 {
		on entry `
			var fs = require('fs');
			process.writeable = fs.createWriteStream('out.log');
			process.stdout.write = process.stderr.write = process.writeable.write.bind(process.writeable);
		`
		state WaitForM1 {
			transition -> WaitForM2
			event e : diversified?m1
			action do
				id = e.a
				println "#APP: Ooh, I needed that app?m1(" , e.a , ", " , e.b , ", " , e.c , ", " , e.d , ", " , e.e , ")!"
			end
		}
		state WaitForM2 {
			transition -> SendAck
			event e : diversified?m2
			action println "#APP: Ooh, I needed that app?m2(" , e.a , ", " , e.b , ", " , e.c , ")!"
		}
		state SendAck {
			on entry do
				println "#APP: Come get some app!m3(" , id , ")!"
				do
					do
						var m3aArg0 : LongLong = id
						println "!" , "44" , ',' , `((` & m3aArg0 & ` >> 56) & 0xFF)` , ',' , `((` & m3aArg0 & ` >> 48) & 0xFF)` , ',' , `((` & m3aArg0 & ` >> 40) & 0xFF)` , ',' , `((` & m3aArg0 & ` >> 32) & 0xFF)` , ',' , `((` & m3aArg0 & ` >> 24) & 0xFF)` , ',' , `((` & m3aArg0 & ` >> 16) & 0xFF)` , ',' , `((` & m3aArg0 & ` >> 8) & 0xFF)` , ',' , `((` & m3aArg0 & ` >> 0) & 0xFF)` , ','
						println ":" , "Byte" , ',' , "LongLong" , ',' , "LongLong" , ',' , "LongLong" , ',' , "LongLong" , ',' , "LongLong" , ',' , "LongLong" , ',' , "LongLong" , ',' , "LongLong" , ','
						println "?" , "0" , ',' , "0" , ',' , "0" , ',' , "0" , ',' , "0" , ',' , "0" , ',' , "0" , ',' , "0" , ',' , "0" , ','
						app!m3a(m3aArg0)
					end
					do
						println "!" , "43" , ','
						println ":" , "Byte" , ','
						println "?" , "0" , ','
						app!m3_()
					end
				end
			end
			transition -> WaitForM1
		}
		region generate_m2_from_m2__and_m2cab init INIT {
			composite state INIT init S1 {
				property c : Integer
				property a : Byte
				property b : Byte
				state S1 @diversify "not" {
					transition -> S2
					event ev : app?m2_
					action do
					end
					transition -> S3
					event ev : app?m2cab
					action do
						c = ev.c
						a = ev.a
						b = ev.b
					end
				}
				state S2 @diversify "not" {
					transition -> S4
					event ev : app?m2cab
					action do
						c = ev.c
						a = ev.a
						b = ev.b
					end
				}
				state S3 @diversify "not" {
					transition -> S4
					event ev : app?m2_
					action do
					end
				}
				state S4 @diversify "not" {
					on entry do
						diversified!m2(c, a, b)
					end
					transition -> S1
				}
			}
		}
		region generate_m1_from_m1d_and_m1abce init INIT {
			composite state INIT init S1 {
				property d : LongLong
				property a : Byte
				property b : Byte
				property c : Integer
				property e : Byte
				state S1 @diversify "not" {
					transition -> S2
					event ev : app?m1d
					action do
						d = ev.d
					end
					transition -> S3
					event ev : app?m1abce
					action do
						a = ev.a
						b = ev.b
						c = ev.c
						e = ev.e
					end
				}
				state S2 @diversify "not" {
					transition -> S4
					event ev : app?m1abce
					action do
						a = ev.a
						b = ev.b
						c = ev.c
						e = ev.e
					end
				}
				state S3 @diversify "not" {
					transition -> S4
					event ev : app?m1d
					action do
						d = ev.d
					end
				}
				state S4 @diversify "not" {
					on entry do
						diversified!m1(d, a, b, c, e)
					end
					transition -> S1
				}
			}
		}
	}
}
thing Client includes Msgs {
	required port app {
		sends m2_ sends m2cab sends m1d sends m1abce receives m3_ receives m3a
	}
	internal port diversified {
		sends m3 receives m3
	}
	property counter : Integer = 0
	property start : Integer
	property _a : Byte
	property _b : Byte
	property _c : Byte
	property _d : Byte
	property _e : Byte
	statechart init RUN {
		on entry do
			_a = rnd()
			_b = 0x02
			start = `new Date()`
		end
		state RUN {
			on entry do
				_c = rnd()% 20
				_d = rnd()% 10 + 20
				_e = rnd()% 5 + 10
				println "#CLI: Come get some app!m1(" , _a , ", " , _b , ", " , _c , ", " , _d , ", " , _e , ")!"
				do
					do
						var m1dArg0 : LongLong = _d
						println "!" , "41" , ',' , `((` & m1dArg0 & ` >> 56) & 0xFF)` , ',' , `((` & m1dArg0 & ` >> 48) & 0xFF)` , ',' , `((` & m1dArg0 & ` >> 40) & 0xFF)` , ',' , `((` & m1dArg0 & ` >> 32) & 0xFF)` , ',' , `((` & m1dArg0 & ` >> 24) & 0xFF)` , ',' , `((` & m1dArg0 & ` >> 16) & 0xFF)` , ',' , `((` & m1dArg0 & ` >> 8) & 0xFF)` , ',' , `((` & m1dArg0 & ` >> 0) & 0xFF)` , ','
						println ":" , "Byte" , ',' , "LongLong" , ',' , "LongLong" , ',' , "LongLong" , ',' , "LongLong" , ',' , "LongLong" , ',' , "LongLong" , ',' , "LongLong" , ',' , "LongLong" , ','
						println "?" , "0" , ',' , "1" , ',' , "1" , ',' , "1" , ',' , "1" , ',' , "1" , ',' , "1" , ',' , "1" , ',' , "1" , ','
						app!m1d(m1dArg0)
					end
					do
						var m1abceArg0 : Byte = _a
						var m1abceArg1 : Byte = _b
						var m1abceArg2 : Integer = _c
						var m1abceArg3 : Byte = _e
						println "!" , "42" , ',' , `((` & m1abceArg0 & ` >> 0) & 0xFF)` , ',' , `((` & m1abceArg1 & ` >> 0) & 0xFF)` , ',' , `((` & m1abceArg2 & ` >> 8) & 0xFF)` , ',' , `((` & m1abceArg2 & ` >> 0) & 0xFF)` , ',' , `((` & m1abceArg3 & ` >> 0) & 0xFF)` , ','
						println ":" , "Byte" , ',' , "Byte" , ',' , "Byte" , ',' , "Integer" , ',' , "Integer" , ',' , "Byte" , ','
						println "?" , "0" , ',' , "0" , ',' , "0" , ',' , "0" , ',' , "0" , ',' , "0" , ','
						app!m1abce(m1abceArg0, m1abceArg1, m1abceArg2, m1abceArg3)
					end
				end
				println "#CLI: Come get some app!m2(" , _a , ", " , _b , ", " , _c , ")!"
				do
					do
						println "!" , "39" , ','
						println ":" , "Byte" , ','
						println "?" , "0" , ','
						app!m2_()
					end
					do
						var m2cabArg0 : Integer = _c
						var m2cabArg1 : Byte = _a
						var m2cabArg2 : Byte = _b
						println "!" , "40" , ',' , `((` & m2cabArg0 & ` >> 8) & 0xFF)` , ',' , `((` & m2cabArg0 & ` >> 0) & 0xFF)` , ',' , `((` & m2cabArg1 & ` >> 0) & 0xFF)` , ',' , `((` & m2cabArg2 & ` >> 0) & 0xFF)` , ','
						println ":" , "Byte" , ',' , "Integer" , ',' , "Integer" , ',' , "Byte" , ',' , "Byte" , ','
						println "?" , "0" , ',' , "0" , ',' , "0" , ',' , "0" , ',' , "0" , ','
						app!m2cab(m2cabArg0, m2cabArg1, m2cabArg2)
					end
				end
				counter ++
			end
			transition -> STOP
			event e : diversified?m3
			guard counter >= 5
			transition -> RUN
			event e : diversified?m3
			guard e.a == _a and counter < 5
			action do
				println "#CLI: Ooh, I needed that app?m3(" , e.a , ")!"
				println "#CLI: We meet again, Doctor Jones!"
			end
			transition -> ERROR
			event e : diversified?m3
			guard e.a != _a
			action do
				println "#CLI: Ooh, I needed that app?m3(" , e.a , ")!"
				println "#CLI: Damn, you re ugly."
			end
		}
		final state STOP {
			on entry do
				println "#CLI: What are you waitin for? Christmas?"
				var duration : Integer = `new Date()` - start
				println "#CLI: took " , duration , "ms."
			`process.writeable.end(() => {
					process.exit(0);
				})`
			end
		}
		final state ERROR {
			on entry do
				println "#CLI: Heh, heh, heh... what a mess!"
			`process.exit(1);`
			end
		}
		region generate_m3_from_m3__and_m3a init INIT {
			composite state INIT init S1 {
				property a : LongLong
				state S1 @diversify "not" {
					transition -> S2
					event ev : app?m3_
					action do
					end
					transition -> S3
					event ev : app?m3a
					action do
						a = ev.a
					end
				}
				state S2 @diversify "not" {
					transition -> S4
					event ev : app?m3a
					action do
						a = ev.a
					end
				}
				state S3 @diversify "not" {
					transition -> S4
					event ev : app?m3_
					action do
					end
				}
				state S4 @diversify "not" {
					on entry do
						diversified!m3(a)
					end
					transition -> S1
				}
			}
		}
	}
}
protocol Default ;

configuration test {
	instance app : App
	instance client1 : Client
	connector client1.app => app . app
}
