thing fragment BasicIAController includes IAController @stdout_sync "true" {
	property ctrlx : Int16 = 0
	statechart SC init Following {
		state Following {
			property game_updateIA_val13 : Byte
			property game_updateIA_pady : Int16
			property game_updateIA_ballx : Int16
			property game_updateIA_bally : Int16
			property game_updateIA_padx : Int16
			property received_game_updateIAval13padyballx : Boolean
			property received_game_updateIAballypadx : Boolean
			on entry do
				readonly var positionArg0_0 : Int16 = ctrlx
				readonly var positionArg1_1 : Int16 = 0
				println "@BasicIAControllerJava@" , "position" , "," , "x" , "," , "x" , "," , "y" , "," , "y" , ","
				do
					do
						readonly var positionval11yArg0_0 : Byte = 0x71
						readonly var positionval11yArg1_1 : Byte = 0xF0
						readonly var positionval11yArg2_2 : Int16 = positionArg1_1
						println "!" , "23" , "," , (`((` & positionval11yArg0_0 & ` >> 0) & 0xFF)`) as Int8 , "," , (`((` & positionval11yArg1_1 & ` >> 0) & 0xFF)`) as Int8 , "," , (`((` & positionval11yArg2_2 & ` >> 8) & 0xFF)`) as Int8 , "," , (`((` & positionval11yArg2_2 & ` >> 0) & 0xFF)`) as Int8 , ","
						controls!positionval11y(positionval11yArg0_0, positionval11yArg1_1, positionval11yArg2_2)
						bytesSentCounter = bytesSentCounter + 5
					end
					do
						readonly var positionxArg0_3 : Byte = 0x55
						readonly var positionxArg1_4 : Int16 = positionArg0_0
						println "!" , "22" , "," , (`((` & positionxArg0_3 & ` >> 0) & 0xFF)`) as Int8 , "," , (`((` & positionxArg1_4 & ` >> 8) & 0xFF)`) as Int8 , "," , (`((` & positionxArg1_4 & ` >> 0) & 0xFF)`) as Int8 , ","
						controls!positionx(positionxArg0_3, positionxArg1_4)
						bytesSentCounter = bytesSentCounter + 4
					end
				end
			end
			internal
			event e : game?updateIAval13padyballx
			guard not received_game_updateIAballypadx
			action do
				received_game_updateIAval13padyballx = true
				game_updateIA_val13 = e.val13
				game_updateIA_pady = e.pady
				game_updateIA_ballx = e.ballx
			end
			internal
			event e : game?updateIAballypadx
			guard not received_game_updateIAval13padyballx
			action do
				received_game_updateIAballypadx = true
				game_updateIA_bally = e.bally
				game_updateIA_padx = e.padx
			end
			transition -> Following
			event e : game?updateIAval13padyballx
			guard received_game_updateIAballypadx
			action do
				game_updateIA_val13 = e.val13
				game_updateIA_pady = e.pady
				game_updateIA_ballx = e.ballx
				do
					if (e.ballx > game_updateIA_padx + 400) ctrlx = ctrlx + 4
					else if (e.ballx < game_updateIA_padx - 400) ctrlx = ctrlx - 4
					if (ctrlx < -100) ctrlx = -100
					else if (ctrlx > 100) ctrlx = 100
				end
				received_game_updateIAval13padyballx = false
				received_game_updateIAballypadx = false
			end
			transition -> Following
			event e : game?updateIAballypadx
			guard received_game_updateIAval13padyballx
			action do
				game_updateIA_bally = e.bally
				game_updateIA_padx = e.padx
				do
					if (game_updateIA_ballx > e.padx + 400) ctrlx = ctrlx + 4
					else if (game_updateIA_ballx < e.padx - 400) ctrlx = ctrlx - 4
					if (ctrlx < -100) ctrlx = -100
					else if (ctrlx > 100) ctrlx = 100
				end
				received_game_updateIAballypadx = false
				received_game_updateIAval13padyballx = false
			end
		}
	}
}
